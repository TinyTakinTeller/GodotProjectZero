[audio]

buses/default_bus_layout="res://resources/audio_settings/default_audio_bus_layout.tres"

[autoload]

Resources="*res://global/autoload/resources.gd"
SaveFile="*res://global/autoload/save_file/save_file.tscn"
Locale="*res://global/autoload/locale/locale.tscn"
Audio="*res://global/autoload/audio/audio.tscn"
SignalBus="*res://global/autoload/signal_bus/signal_bus.tscn"
Scene="*res://global/autoload/scene/scene.tscn"
Spawning="*res://addons/BulletUpHell/Spawning.tscn"

[debug]

gdscript/warnings/unused_signal=0
gdscript/warnings/untyped_declaration=1
